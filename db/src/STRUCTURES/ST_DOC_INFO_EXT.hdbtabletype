type "ST_DOC_INFO_EXT" AS TABLE (
  FILE_ID NVARCHAR(36) NOT NULL,
  PO_FILE_NO BIGINT,
  DOC_TYPE NVARCHAR(50),
  AMOUNT DECIMAL,   
  FILE_CONTENT BLOB,
  FILE_MIME_TYPE NVARCHAR(2000),
  FILE_NAME NVARCHAR(1000),
  INSTANCE_UID NVARCHAR(36),
  INVOICE_NO BIGINT,
  INVOICE_DATE DATE,
  STATUS NVARCHAR(100),
  UPLOADED_ON TIMESTAMP,
  SAP_DOCUMENT_NO NVARCHAR(100),
  LOGIN_ID NVARCHAR(500),    
  PRIMARY KEY(FILE_ID)
)