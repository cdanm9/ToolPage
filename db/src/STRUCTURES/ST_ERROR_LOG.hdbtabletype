type "ST_ERROR_LOG" AS TABLE(
	 LOG_ID NVARCHAR (50) NOT NULL,
     REG_NO BIGINT ,
     SR_NO BIGINT,
	 ERROR_CODE BIGINT ,
	 ERROR_DESCRPTION NVARCHAR(1000) ,    
	 CREATED_ON TIMESTAMP ,
	 USER_ID NVARCHAR(50),    
	 APP_NAME NVARCHAR(50),   
	 TYPE NVARCHAR (50), 
	PRIMARY KEY(LOG_ID)
)

 
   
	
 
