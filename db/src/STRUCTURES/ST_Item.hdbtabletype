type "ST_Item"  AS TABLE(
    ORDER_NO INTEGER NOT NULL,
    ITEM INTEGER NOT NULL,
    CLASS NVARCHAR(40),
    MATERIAL NVARCHAR(100),
    QUANTITY INTEGER,
    UNIT_PRICE DECIMAL(16,3),
    TOTAL_PRICE DECIMAL(16,3),
	CURRENCY NVARCHAR(10),
	RKIT INTEGER,
    ACKIT INTEGER,
    GKIT INTEGER,
    PRIMARY KEY(ORDER_NO,ITEM)
)

