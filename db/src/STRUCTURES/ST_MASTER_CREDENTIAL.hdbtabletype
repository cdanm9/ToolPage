type "ST_MASTER_CREDENTIAL"  AS TABLE(

    SR_NO  INTEGER NOT NULL ,
    USERNAME  NVARCHAR(100)  NOT NULL ,
    PASSWORD  NVARCHAR(100)  NOT NULL ,
    TYPE  NVARCHAR(100),
    ADD_INFO1  NVARCHAR(100)  ,
    ADD_INFO2  NVARCHAR(100)  ,
    ADD_INFO3  NVARCHAR(100)  ,
    CREATED_ON  TIMESTAMP,
   PRIMARY KEY(SR_NO)
)