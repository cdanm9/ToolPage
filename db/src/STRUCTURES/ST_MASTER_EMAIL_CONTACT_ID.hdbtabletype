    EMAIL_NOTIF_1 NVARCHAR(100)   NOT NULL,
    EMAIL_NOTIF_2 NVARCHAR(100)  NOT NULL,
    EMAIL_NOTIF_3 NVARCHAR(100)  NOT NULL,
    CONTACT_ID_1 NVARCHAR(100) NOT NULL,
    CONTACT_ID_2 NVARCHAR(100)  NOT NULL,
    CONTACT_ID_3 NVARCHAR(100)  NOT NULL,
    CLIENT_FULL_NAME NVARCHAR(100)   NOT NULL,
    CLIENT_SHORT_NAME NVARCHAR(100)   NOT NULL,
    CLIENT_COUNTRY NVARCHAR(100)  NOT NULL,
PRIMARY KEY(SR_NO)
)
