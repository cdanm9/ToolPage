type "ST_MASTER_IVEN_ATTACHMENTS" as TABLE(

     SR_NO INTEGER NOT NULL,
     ENTITY_CODE NVARCHAR(10) NOT NULL,
     ATTACH_CODE INTEGER NOT NULL,
    
     ATTACH_GROUP NVARCHAR(30),
     ATTACH_DESC NVARCHAR(100),
    
     FILE_NAME NVARCHAR(100),
     FILE_TYPE NVARCHAR(100),
     FILE_MIMETYPE NVARCHAR(100),
     FILE_CONTENT BLOB,
     UPLOADED_ON TIMESTAMP,
     ATTACH_TYPE_CODE NVARCHAR(10),
     ATTACH_TYPE_DESC NVARCHAR(100),

PRIMARY KEY(SR_NO,ENTITY_CODE,ATTACH_CODE)
)