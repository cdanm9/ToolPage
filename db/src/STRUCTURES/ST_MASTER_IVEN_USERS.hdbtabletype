type "ST_MASTER_IVEN_USERS" AS TABLE(

     SR_NO INTEGER ,
     USER_ID NVARCHAR(50),
     USER_ROLE NVARCHAR(50) ,
     USER_NAME NVARCHAR(500) ,
     EMAIL NVARCHAR(150) ,
     COMPANY_CODE NVARCHAR (500) ,
     EMP_NO NVARCHAR (100) ,
     CREATED_ON TIMESTAMP,
	 UPDATED_ON TIMESTAMP,
	 ACTIVE NVARCHAR(1),
PRIMARY KEY(SR_NO,EMAIL)
)