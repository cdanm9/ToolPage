type "ST_MASTER_TABLENAMES" AS TABLE(
   SR_NO INTEGER NOT NULL,
   TABLE_CODE NVARCHAR(25) NOT NULL,
   TABLE_NAME NVARCHAR(50) NOT NULL,
   TABLE_TYPE NVARCHAR(25) NOT NULL,
   COLUMN_COUNT INTEGER NOT NULL,
   TABLE_DESCRIPTION NVARCHAR(100),
   PRIMARY_KEY NVARCHAR(300),
   PRIMARY KEY(SR_NO)
)

   
 