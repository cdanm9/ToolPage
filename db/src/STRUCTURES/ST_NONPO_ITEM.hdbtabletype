TYPE "ST_NONPO_ITEM" AS TABLE ( 
  REQUEST_NO INTEGER NOT NULL,
  INVOICE_ITEM INTEGER NOT NULL,
  GL_CODE NVARCHAR(10),
  RATE DOUBLE,
  HSNCODE NVARCHAR(10),
  GST NVARCHAR(10),
  PROFIT_CENTER NVARCHAR(10),
  SHIP_TO_LOCATION NVARCHAR(50),
  COST_CENTER NVARCHAR(10),
  COST_ALLOCATION NVARCHAR(10),
  GL_DESCRIPTION NVARCHAR(100),
  PRIMARY KEY(REQUEST_NO, INVOICE_ITEM)
)