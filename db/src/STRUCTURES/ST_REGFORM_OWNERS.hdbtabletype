type "ST_REGFORM_OWNERS" AS TABLE(
     REQUEST_NO BIGINT NOT NULL,
     SR_NO INTEGER NOT NULL,
     NAME NVARCHAR(100),
     NATIONALITY NVARCHAR(100),
     CONTACT_NO NVARCHAR(12),
     CONTACT_TELECODE NVARCHAR(4),
     PASSPORT_NO NVARCHAR(30),
     ACTIVITY_TYPE NVARCHAR(30),
     OWNERSHIP_PERCENT DOUBLE,
PRIMARY KEY(REQUEST_NO,SR_NO)
)

	