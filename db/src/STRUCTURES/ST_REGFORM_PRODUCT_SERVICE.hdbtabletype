type "ST_REGFORM_PRODUCT_SERVICE" AS TABLE(
    REQUEST_NO BIGINT NOT NULL,
    TYPE NVARCHAR(10),
    SR_NO INTEGER NOT NULL,
    
    PROD_NAME NVARCHAR(100),
    PROD_DESCRIPTION NVARCHAR(200),
    PROD_CATEGORY NVARCHAR(200),
    PROD_CATEGORY_DEC NVARCHAR(100),

PRIMARY KEY(REQUEST_NO,TYPE,SR_NO)
)
