type "ST_SUPPLIER_PROFILE_LOG" AS TABLE(

     SAP_VENDOR_CODE NVARCHAR(10),
     EVENT_NO INTEGER NOT NULL,
     EVENT_CODE INTEGER NOT NULL,
     EVENT_TYPE NVARCHAR(20),
	
	 USER_ID NVARCHAR(100),
	 USER_NAME NVARCHAR(100),
	 REMARK NVARCHAR(100),
	 COMMENT NVARCHAR(1000),
	 UPDATED_ON TIMESTAMP,
	 UPDATED_FIELD_NAME NVARCHAR(100),
	 CHANGE_VALUE NVARCHAR(100),
	 ORG_VALUE NVARCHAR(100),
	 OBR_NO BIGINT,
PRIMARY KEY(
SAP_VENDOR_CODE,EVENT_NO
))