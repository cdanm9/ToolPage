type "ST_USER_DELEGATION_HISTORY" AS TABLE(
    SR_NO INTEGER NOT NULL,
    LOG_NO INTEGER NOT NULL,
    ASSIGN_FROM NVARCHAR(50),
    ASSIGN_TO NVARCHAR(50),
	ASSIGN_TO_NAME NVARCHAR(100) NOT NULL,
	REASON NVARCHAR(1000) NOT NULL,
	DEL_FROM_DATE TIMESTAMP,
	DEL_TO_DATE TIMESTAMP,
	STATUS NVARCHAR(1),
	ENTITY_CODE NVARCHAR(10),
	CHANGED_BY NVARCHAR(50),
	CHANGED_DATE TIMESTAMP,
	PRIMARY KEY(SR_NO, LOG_NO)
)