{
  "name": "VENDOR_PORTAL",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js-community/odata-v2-adapter": "^1.11.6",
    "@sap-cloud-sdk/core": "^1.54.2",
    "@sap-cloud-sdk/http-client": "^3.4.0",
    "@sap-cloud-sdk/resilience": "^3.4.0",
    "@sap/cds": "^7.3.1",
    "@sap/cds-hana": "^2",
    "@sap/hana-client": "^2.19.21",
    "@sap/hdbext": "^7.7.6",
    "core-util-is": "^1.0.3",
    "crypto": "^1.0.1",
    "express": "^4",
    "nodemailer": "^6.9.7",
    "npm": "^10.2.5",
    "passport": "^0.6.0",
    "sap-cf-axios": "^0.4.10",
    "sap-hdbext-promisfied": "2.202307.1"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^1",
    "@sap/cds-dk": "^7",
    "@sap/hdi-deploy": "^4.8.0"
  },
  "scripts": {
    "start": "cds-serve"
  },
  "cds": {
    "requires": {
      "db": "hana",
      "ERP": {
        "kind": "rest",
        "credentials": {
          "destination": "ERP",
          "path": "/sap/opu/odata/sap/"
        }
      },
      "EMAIL_DEST": {
        "kind": "rest",
        "credentials": {
          "destination": "EMAIL_DEST",
          "path": "/iVenEmail_V2"
        }
      },
      "IAS_DEST": {
        "kind": "rest",
        "credentials": {
          "destination": "IAS_DEST",
          "path": "/scim"
        }
      },
      "ZIVN_VENDOR_REG_SRV": {
        "kind": "odata-v2",
        "model": "srv/external/ZIVN_VENDOR_REG_SRV",
        "csrf": true,
        "csrfInBatch": true,
        "credentials": {
          "destination": "ERP",
          "path": "sap/opu/odata/sap/ZIVN_VENDOR_REG_SRV",
          "forwardAuthToken": true,
          "timeout": 100000
        }
      },
      "ZMDG_VENDOR_REG_SRV": {
        "kind": "rest",
        "model": "srv/external/ZMDG_VENDOR_REG_SRV",
        "csrf": true,
        "csrfInBatch": true,
        "credentials": {
          "destination": "ERP",
          "path": "/sap/opu/odata/sap/ZMDG_VENDOR_REG_SRV"
        }
      },
      "BTP_DMS_Token": {
        "kind": "rest",
        "credentials": {
          "destination": "BTP_DMS_Token",
          "path": "/oauth/token"
        }
      },
      "BTP_DMS_Dest": {
        "kind": "rest",
        "credentials": {
          "destination": "BTP_DMS_Dest",
          "path": ""
        }
      },
      "auth": {
        "kind": "mocked"
      },
      "connectivity": true
    },
    "hana": {
      "deploy-format": "hdbtable"
    },
    "cov2ap": {
      "plugin": true
    }
  }
}